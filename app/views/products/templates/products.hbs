<div class="container">

    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{company}}
                <small>I.S. Pharma</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#">Inicio</a>
                </li>
                <li class="active">Productos {{company}}</li>
            </ol>
        </div>
    </div>


    <div class="row">
        {{#each categories}}
            <div class="col-md-4 img-portfolio category-container" data-category="{{category}}">
                <a href="#"><img class="img-responsive img-hover category-company" src="{{image}}" style="height: 272px;"></a>
                <h3><a href="#" class="category-company">{{category}}</a></h3>
            </div>
         {{/each}}
    </div>

    <hr>

</div>
